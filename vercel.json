{
	"version": 2,
	"builds": [
		{
			"src": "backend/api/index.js",
			"use": "@vercel/node"
		},
		{
			"src": "frontend/travel-story-app/dist/**/*",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "backend/api/index.js"
		},
		{
			"src": "/(.*)",
			"dest": "frontend/travel-story-app/dist/$1"
		}
	]
}
